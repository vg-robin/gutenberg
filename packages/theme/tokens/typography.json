{
	"font": {
		"family": {
			"$type": "fontFamily",
			"heading": {
				"$value": [
					"-apple-system",
					"system-ui",
					"Segoe UI",
					"Roboto",
					"Oxygen-Sans",
					"Ubuntu",
					"Cantarell",
					"Helvetica Neue",
					"sans-serif"
				],
				"$description": "Headings font family"
			},
			"body": {
				"$value": [
					"-apple-system",
					"system-ui",
					"Segoe UI",
					"Roboto",
					"Oxygen-Sans",
					"Ubuntu",
					"Cantarell",
					"Helvetica Neue",
					"sans-serif"
				],
				"$description": "Body font family"
			},
			"mono": {
				"$value": [ "Menlo", "Consolas", "monaco", "monospace" ],
